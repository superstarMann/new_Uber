{
    "noImplicitAny": true,
    "compilerOptions": {
    "baseUrl": "../../../",
    "paths": {
    "jquery": ["node_modules/jquery/dist/jquery"]
    },
    "typeRoots": [
    "node_modules/@types"
    ],
    "module": "commonjs",
    "target": "es5",
    "lib": [ "es2015", "dom" ],
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
    },
    "exclude": [
    "node_modules"
    ]
    }